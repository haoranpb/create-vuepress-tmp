{
  "name": "create-vuepress-tmp",
  "version": "0.2.1",
  "description": "A fork of create-vuepress with some updates",
  "keywords": [
    "vuepress",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "ludanxer/create-vuepress-tmp"
  },
  "license": "MIT",
  "author": "ludanxer <ludanxer@icloud.com>",
  "bin": {
    "create-vuepress-tmp": "index.js"
  },
  "files": [
    "/lib",
    "index.js"
  ],
  "scripts": {
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "lint-staged": {
    "*.{js}": "eslint --fix",
    "package.json": "sort-package-json"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "debug": "^4.1.1",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "inquirer": "^7.1.0",
    "lodash.template": "^4.5.0",
    "semver": "^7.1.3",
    "shelljs": "^0.8.3",
    "yargs-parser": "^18.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "sort-package-json": "^1.40.0",
    "standard-version": "^7.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
